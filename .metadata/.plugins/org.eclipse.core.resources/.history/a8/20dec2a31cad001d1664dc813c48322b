package com.ctl.app.entity;

import jakarta.persistence.Entity;
import lombok.AllArgsConstructor;
import lombok.NoArgsConstructor;

@En
@AllArgsConstructor
@NoArgsConstructor
public class RoomList {
	
	private String roomNo;
	private long rent;
	private int maintenenceCost;
	private String availaility;
	public String getRoomNo() {
		return roomNo;
	}
	public void setRoomNo(String roomNo) {
		this.roomNo = roomNo;
	}
	public long getRent() {
		return rent;
	}
	public void setRent(long rent) {
		this.rent = rent;
	}
	public int getMaintenenceCost() {
		return maintenenceCost;
	}
	public void setMaintenenceCost(int maintenenceCost) {
		this.maintenenceCost = maintenenceCost;
	}
	public String getAvailaility() {
		return availaility;
	}
	public void setAvailaility(String availaility) {
		this.availaility = availaility;
	}

	
	
}
